{
  "get": {
    "summary": "Get All TLDs (Alias)",
    "description": "Get all the Top Level Domains/Namespaces of the Alias system",
    "tags": [
      "alias"
    ],
    "parameters": [
      {
        "$ref": "../../parameters/timestamp.json"
      },
      {
        "$ref": "../../parameters/firstIndex.json"
      },
      {
        "$ref": "../../parameters/lastIndex.json"
      }
    ],
    "responses": {
